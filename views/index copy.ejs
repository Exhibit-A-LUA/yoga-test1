<%- include('layout/header'); %>

<div class="container">
  <div class="row my-4">
    <div class="col-lg-12">
      <% if (message) { %>
        <div class="alert alert-dismissable fade show alert-<%= message.type %>" role="alert">
          <button class="btn-close" type="button" data-bs-dismiss="alert" aria-label="Close"></button>
          <strong><%= message.message %></strong>
        </div>
      <% } %>
      <div class="table-responsive">
        <table class="table table-striped text-center">
          <thead>
            <tr class="table-dark">
              <th>Num</th>
              <th>Image</th>
              <th>Name</th>
              <th>Type</th>
              <th>Package</th>
              <th>Start</th>
              <th>Action</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>2</td>
              <td><img src="" alt=""></td>
              <td>Andrew</td>
              <td>yoga</td>
              <td>12</td>
              <td>09/09/2001</td>
              <td>
                <a href="" class="text-success"><i class="fas fa-edit fa-lg mx-1"></i></a>
                <a href="" class="text-danger"><i class="fas fa-trash fa-lg mx-1"></i></a>
              </td>
            </tr>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>

<%- include('layout/footer'); %>
